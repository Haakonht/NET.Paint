<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <PathGeometry x:Key="HalfGeometry">
        <PathFigure StartPoint="0,50" IsClosed="True">
            <ArcSegment Point="100,50" Size="50,50" SweepDirection="Clockwise" IsLargeArc="False"/>
            <LineSegment Point="100,50" />
            <LineSegment Point="0,50" />
        </PathFigure>
    </PathGeometry>


    <PathGeometry x:Key="ThirdsGeometry">
        <PathFigure StartPoint="50,0">
            <PathFigure.Segments>
                <LineSegment Point="50,0"/>
                <ArcSegment Point="93.3,75" SweepDirection="Clockwise" Size="50,50"/>
                <LineSegment Point="50,50"/>
            </PathFigure.Segments>
        </PathFigure>
    </PathGeometry>


    <PathGeometry x:Key="QuarterGeometry">
        <PathFigure StartPoint="0,50" IsClosed="True">
            <ArcSegment Point="50,0" Size="50,50" SweepDirection="Clockwise" IsLargeArc="False"/>
            <LineSegment Point="50,0"/>
            <LineSegment Point="50,50"/>
        </PathFigure>
    </PathGeometry>

    <EllipseGeometry x:Key="CenterGeometry" Center="50,50" RadiusX="20" RadiusY="20"/>
    <CombinedGeometry x:Key="CircleHalves" GeometryCombineMode="Exclude" Geometry1="{StaticResource HalfGeometry}" Geometry2="{StaticResource CenterGeometry}"/>
    <CombinedGeometry x:Key="CircleThirds" GeometryCombineMode="Exclude" Geometry1="{StaticResource ThirdsGeometry}" Geometry2="{StaticResource CenterGeometry}"/>
    <CombinedGeometry x:Key="CircleQuarters" GeometryCombineMode="Exclude" Geometry1="{StaticResource QuarterGeometry}" Geometry2="{StaticResource CenterGeometry}"/>
</ResourceDictionary>